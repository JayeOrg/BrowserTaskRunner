{
  "name": "sitecheck",
  "version": "1.0.0",
  "private": true,
  "description": "Cloudflare bypass solutions for automated login",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "check": "docker-compose -f solutions/docker/docker-compose.yml --env-file .env up --build",
    "monitor": "node dist/src/botc-login.js",
    "extension": "node dist/solutions/extension/index.js",
    "dev:monitor": "tsc && node dist/src/botc-login.js",
    "dev:extension": "tsc && node dist/solutions/extension/index.js",
    "docker:build": "docker-compose -f solutions/docker/docker-compose.yml --env-file .env build",
    "docker:up": "docker-compose -f solutions/docker/docker-compose.yml --env-file .env up",
    "docker:down": "docker-compose -f solutions/docker/docker-compose.yml down"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@types/ws": "^8.18.1",
    "typescript": "^5.9.3"
  }
}
